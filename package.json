{
  "name": "@ralph/ralph-storefront",
  "version": "1.0.0",
  "description": "E-commerce frontend by Carismar",
  "author": "Olivia Axelsson & Lars Johansson",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "set NODE_ENV=production && nuxt build --modern=server",
    "build-local": "set NODE_ENV=development && nuxt build --modern=server",
    "start": "set NODE_ENV=production && nuxt start --modern=server",
    "start-local": "set NODE_ENV=development && nuxt start --modern=server",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "create-component": "node ./scripts/create-component.cjs",
    "create-index-files": "node ./scripts/create-index-files.cjs",
    "stats": "set NODE_ENV=development && yarn nuxt build --modern=server --analyze"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/apollo": "4.0.1-rc.4",
    "@nuxtjs/component-cache": "^1.1.6",
    "@nuxtjs/pwa": "^3.3.4",
    "@ralph/ralph-ui": "latest",
    "cookie": "^0.4.1",
    "cookie-universal-nuxt": "^2.1.4",
    "express": "^4.17.1",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.12",
    "nuxt-i18n": "^6.18.0",
    "nuxt-user-agent": "^1.2.2",
    "vue-slide-up-down": "^2.0.1",
    "vuex-persistedstate": "^3.2.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "core-js": "^3.8.3",
    "csv-parser": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.3.1",
    "file-save": "^0.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "prettier": "^1.19.1",
    "prompts": "^2.4.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "svg-inline-loader": "^0.8.2",
    "uppercamelcase": "^3.0.0",
    "vue-jest": "^4.0.0-rc.1"
  }
}
